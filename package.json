    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "faker": "4.1.0",
    "husky": "4.2.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "rxjs": "6.4.0",
    "stylelint": "13.0.0",
    "stylelint-scss": "3.14.2",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
